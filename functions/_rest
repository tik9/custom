}


_pt(){
	_alternative 'Nummer:pts:((n\:"pts-endnr."))'
}

_sc2(){
	_arguments '-d[zielverzeichnis]:verz:_files -/' '-f[quell datei]:dateiname:_files' '-p[port]' '-o[letztes Oktett]' '-ip[ip-basis]' 
}


_schieb(){
